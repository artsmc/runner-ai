  "scripts": {
    "heroku-prebuild": "rm -rf node_modules && npm cache clean --force && export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "postinstall": "npm run build",
    "build:client:dev": "webpack --config webpack.client.config.js --mode development",
    "build:server:dev": "webpack --config webpack.server-dev.config.js --mode development",
    "build:client:prod": "webpack --config webpack.client.config.js --mode production",
    "build:server:prod": "webpack --config webpack.server.config.js --mode production",
    "build:dev": "npm run build:server:dev && npm run build:client:dev",
    "build": "npm run build:server:prod && npm run build:client:prod",
    "start": "node ./dist/server/server.js --PORT 8080",
    "start:dev": "npm-run-all --parallel  watch",
    "watch": "nodemon  -e ts,js,css,scss,hbs -w ./app -x npm run hotload",
    "hotload": "npm run build:dev && node ./dist/server/server.js --PORT 8090"
  },
  "devDependencies": {
